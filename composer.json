{
    "name": "rast/activity-log",
    "description": "A simple activity log package that logs to file with model trait and log viewer UI",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Raihan Afroz",
            "email": "raihanafroz9@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RAST\\ActivityLog\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RAST\\ActivityLog\\ActivityLogServiceProvider"
            ],
            "aliases": {
                "ActivityLog": "RAST\\ActivityLog\\Facades\\ActivityLog"
            }
        }
    }
}
